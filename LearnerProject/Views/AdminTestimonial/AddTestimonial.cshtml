@model LearnerProject.Models.Entitities.Teslimonial
@{
    ViewBag.Title = "AddTestimonial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Referans Ekleme Sayfasi</h2>

<form method="post">

    <label>Ad Soyad</label>
    @Html.TextBoxFor(x => x.NameSurname, new { @class = "form-control" })
    <br />

    <label>Unvan</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />

    <label>Resim Url</label>
    @Html.TextBoxFor(x => x.ImageUrl, new { @class = "form-control" })
    <br />

    <label>Aciklama</label>
    @Html.TextAreaFor(x => x.Description, new { @class = "form-control", rows = 5 })
    <br />

    <button class="btn btn-outline-primary">KAYDET</button>
</form>

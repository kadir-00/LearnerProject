@model LearnerProject.Models.Entitities.Contact
@{
    ViewBag.Title = "AddContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Iletisim Ekleme Sayfasi</h2>

<form method="post">

    <label>Adres (Add)</label>
    @Html.TextBoxFor(x => x.Add, new { @class = "form-control" })
    <br />

    <label>Saatler</label>
    @Html.TextBoxFor(x => x.OpenHours, new { @class = "form-control" })
    <br />

    <label>Email</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />

    <label>Telefon</label>
    @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
    <br />

    <button class="btn btn-outline-primary">KAYDET</button>
</form>
